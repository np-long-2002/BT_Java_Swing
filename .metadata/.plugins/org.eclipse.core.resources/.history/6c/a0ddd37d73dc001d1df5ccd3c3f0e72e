package model;

public class Sach {
	private int id;
	public Sach() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Sach(int id, String tieuDe, String tacGia, int namSX, int theLoai, String moTa) {
		super();
		this.id = id;
		this.tieuDe = tieuDe;
		this.tacGia = tacGia;
		this.namSX = namSX;
		this.theLoai = theLoai;
		this.moTa = moTa;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getTieuDe() {
		return tieuDe;
	}
	public void setTieuDe(String tieuDe) {
		this.tieuDe = tieuDe;
	}
	public String getTacGia() {
		return tacGia;
	}
	public void setTacGia(String tacGia) {
		this.tacGia = tacGia;
	}
	public int getNamSX() {
		return namSX;
	}
	public void setNamSX(int namSX) {
		this.namSX = namSX;
	}
	public int getTheLoai() {
		return theLoai;
	}
	public void setTheLoai(int theLoai) {
		this.theLoai = theLoai;
	}
	public String getMoTa() {
		return moTa;
	}
	public void setMoTa(String moTa) {
		this.moTa = moTa;
	}
	private String tieuDe;
	private String tacGia;
	private int namSX;
	private int theLoai;
	private String moTa;
}
